{
  "name": "react-webview-extension",
  "displayName": "React Webview + Note Tags",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:extension.showReactWebview",
    "onCommand:noteTags.refresh",
    "onCommand:noteTags.filterByTag",
    "onView:noteTags",
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.showReactWebview",
        "title": "Show React Webview"
      },
      {
        "command": "noteTags.refresh",
        "title": "Refresh Note Tags"
      },
      {
        "command": "noteTags.filterByTag",
        "title": "Filter Notes by Tag"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "noteTags",
          "name": "Notes by Tag"
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "dev": "webpack --mode development --watch",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "package": "vsce package",
    "prepublish": "npm run build"
  },
 feat/color-picker


  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack"
    "watch": "webpack --watch"
  },

  "categories": [
    "Other"
  ],
main
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "webpack-cli": "^6.0.1",
feat/color-picker
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "vsce": "^2.25.0"

    "webpack-dev-server": "^5.2.2"
main
  }
}
